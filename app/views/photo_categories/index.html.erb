<h1>Listing photo_categories</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Identifier</th>
  </tr>

<% for photo_category in @photo_categories %>
  <tr>
    <td><%=h photo_category.title %></td>
    <td><%=h photo_category.identifier %></td>
    <td><%= link_to 'Show', photo_category %></td>
    <td><%= link_to 'Edit', edit_photo_category_path(photo_category) if is_admin?%></td>
    <td><%= link_to 'Destroy', photo_category, :confirm => 'Are you sure?', :method => :delete if is_admin?%></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New photo_category', new_photo_category_path %>
