<h1>Listing camera_brands</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% for camera_brand in @camera_brands %>
  <tr>
    <td><%=h camera_brand.name %></td>
    <td><%= link_to 'Show', camera_brand %></td>
    <td><%= link_to 'Edit', edit_camera_brand_path(camera_brand) %></td>
    <td><%= link_to 'Destroy', camera_brand, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New camera_brand', new_camera_brand_path %>
